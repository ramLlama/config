# Setup PATH
export PATH=$(fish -c 'echo $PATH' | tr ' ' ':')

# Daemons
/usr/bin/start-pulseaudio-x11
/usr/bin/lockrun -w 5 /run/user/`id -u $USER`/feh-wallpaper-shuffle.sh.lock /home/ram/scripts/feh-wallpaper-shuffle.sh /home/ram/wallpapers 15m &

# Keybindings
/usr/bin/lockrun -w 5 /run/user/`id -u $USER`/xbindkeys.lock xbindkeys -n &

# Dropbox
/usr/bin/dropboxd &

# redshift
/usr/bin/lockrun -w 5 /run/user/`id -u $USER`/redshift.lock redshift -l 40.4428:79.9532 -t 5500:5000 &
